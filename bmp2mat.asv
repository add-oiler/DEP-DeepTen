clear
close all

rgb = imread('Healthy.bmp');
shape = size(rgb);
hs = zeros(shape(1),shape(2),shape(3));

for i = 1:
    for j = 1:243
        is_bg = 1;
        for k = 1:3
            if rgb(i,j,k) ~= 255
                is_bg = 0;
                break;
            end
        end
        if is_bg == 1
            for k = 1:3
                hs(i,j,k) = 1;
            end
        end
    end
end
